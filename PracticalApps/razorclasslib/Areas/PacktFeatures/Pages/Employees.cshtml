@page
@using Northwind.Common.Models;
@model razorclasslib.MyFeature.Pages.EmployeesPageModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<div class="row">
    <h1 class="display-2">Employees</h1>
</div>

<div class="row">
    @foreach (Employee emp in Model.Employees)
    {
        <div class="col-sm-3">
            <partial name="_Employee" model="emp" />
        </div>
    }
</div>